generator client {
  provider = "prisma-client"
  output   = "../src/main/generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model User {
  id       String @id @default(uuid())
  username String @unique
  password String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model Product {
  id String @id @default(uuid())
  name String 
}
